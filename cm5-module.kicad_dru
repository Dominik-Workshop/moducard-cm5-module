(version 1)

(rule "clearance PCIe vs main GND zone (outer layers)"
    (constraint clearance (min 2mm))
    (condition "(A.Type == 'Zone' && A.Name == 'GND_MAIN' && B.NetClass == 'PCIe_85Ohm_diff' && (B.Layer == 'F.Cu' || B.Layer == 'B.Cu')) || (B.Type == 'Zone' && B.Name == 'GND_MAIN' && A.NetClass == 'PCIe_85Ohm_diff' && (A.Layer == 'F.Cu' || A.Layer == 'B.Cu'))")
)

